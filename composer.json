{
    "name": "makise-co/redis",
    "description": "Makise-Co Adapter of PHP Redis extenstion",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "makise-co",
        "redis"
    ],
    "authors": [
        {
            "name": "Dmitry K.",
            "email": "coder1994@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "predis/predis": "For Redis Sentinel support"
    },
    "require": {
        "php": ">=7.4",
        "ext-swoole": ">=4.4",
        "ext-redis": ">=4.4",
        "makise-co/pool": "^1.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.5",
        "phpunit/phpunit": "^9.1",
        "symfony/var-dumper": "^5.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "MakiseCo\\Redis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakiseCo\\Redis\\Tests\\": "tests/"
        }
    }
}
